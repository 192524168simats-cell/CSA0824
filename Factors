X=int(input("enter the number:"))
Y=[]
print("the factor of",X,"are:")
for i in range (1,X):
    if X%i==0:
        Y.append(i)
print(Y)
print("number of factors:",len(Y))
n=int(input("enter N valve:"))
if n*len(Y):
   print("invaild")
else:
    print("first",n,"factors:")
    for k in range (0,n):
        print("Y[k],end=")
